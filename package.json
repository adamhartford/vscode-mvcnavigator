{
  "name": "vscode-mvcnavigator",
  "displayName": "ASP.NET MVC Navigator",
  "description": "Navigate between controllers, views, actions, and view components in ASP.NET MVC projects, similar to JetBrains Rider and ReSharper.",
  "version": "0.0.10",
  "publisher": "adamhartford",
  "author": "Adam Hartford",
  "icon": "icon.png",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/adamhartford/vscode-mvcnavigator.git"
  },
  "homepage": "https://github.com/adamhartford/vscode-mvcnavigator",
  "bugs": {
    "url": "https://github.com/adamhartford/vscode-mvcnavigator/issues"
  },
  "keywords": [
    "asp.net",
    "mvc",
    "navigation",
    "controller",
    "view",
    "action",
    "viewcomponent",
    "csharp",
    "c#",
    "dotnet",
    "jetbrains",
    "resharper",
    "rider"
  ],
  "engines": {
    "vscode": "^1.99.3"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:csharp",
    "onLanguage:razor",
    "onLanguage:aspnetcorerazor",
    "onLanguage:html"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vscode-mvcnavigator.navigateToView",
        "title": "Navigate to View"
      },
      {
        "command": "vscode-mvcnavigator.navigateToAction",
        "title": "Navigate to Action Method"
      },
      {
        "command": "vscode-mvcnavigator.navigateToController",
        "title": "Navigate to Controller Class"
      }
    ],
    "configuration": {
      "title": "ASP.NET MVC Navigator",
      "properties": {
        "mvcNavigator.enableDebugLogging": {
          "type": "boolean",
          "default": false,
          "description": "Enable debug logging for troubleshooting navigation issues. Logs will appear in the developer console."
        }
      }
    }
  },
  "dependencies": {
    "glob": "^10.3.10"
  },
  "scripts": {
    "compile": "npm run check-types && node esbuild.js",
    "check-types": "tsc --noEmit",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "vscode:prepublish": "npm run package",
    "package": "npm run check-types && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir dist",
    "pretest": "npm run compile-tests",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/vscode": "^1.99.3",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "esbuild": "^0.25.10",
    "eslint": "^9.32.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.9.2"
  }
}
