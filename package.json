{
  "name": "vscode-mvcnavigator",
  "displayName": "ASP.NET MVC Navigator",
  "description": "Navigate between controllers, views, and actions in ASP.NET MVC projects.",
  "version": "0.0.2",
  "publisher": "adamhartford",
  "author": "Adam Hartford",
  "icon": "icon.png",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/adamhartford/vscode-mvcnavigator.git"
  },
  "homepage": "https://github.com/adamhartford/vscode-mvcnavigator",
  "bugs": {
    "url": "https://github.com/adamhartford/vscode-mvcnavigator/issues"
  },
  "keywords": [
    "asp.net",
    "mvc",
    "navigation",
    "controller",
    "view",
    "action",
    "csharp",
    "c#",
    "dotnet"
  ],
  "engines": {
    "vscode": "^1.103.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:csharp",
    "onLanguage:razor",
    "onLanguage:aspnetcorerazor",
    "onLanguage:html"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vscode-mvcnavigator.navigateToView",
        "title": "Navigate to View"
      },
      {
        "command": "vscode-mvcnavigator.navigateToAction",
        "title": "Navigate to Action Method"
      },
      {
        "command": "vscode-mvcnavigator.navigateToController",
        "title": "Navigate to Controller Class"
      }
    ],
    "configuration": {
      "title": "ASP.NET MVC Navigator",
      "properties": {
        "mvcNavigator.enableDebugLogging": {
          "type": "boolean",
          "default": false,
          "description": "Enable debug logging for troubleshooting navigation issues. Logs will appear in the developer console."
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.103.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "eslint": "^9.32.0",
    "typescript": "^5.9.2",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2"
  }
}
